    #- COLUMNS:
    #     - attr1:
    - ERROR_LIMIT: {ERROR_LIMIT}
   EXTERNAL:
    - SCHEMA: '%'
   OUTPUT:
    - TABLE: {TABLE}
    - MODE: INSERT
