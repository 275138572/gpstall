HOST: {GPHOST}
PORT: {GPPORT}
GPLOAD:
   INPUT:
    - SOURCE:
         LOCAL_HOSTNAME:
           - {GPDHOST}
         PORT: {GPDPORT}
         FILE:{FILE}
    - FORMAT: csv
    - QUOTE: '"'
    {HEADER}
